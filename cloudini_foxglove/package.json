    "create-foxglove-extension": "1.0.4",
    "eslint": "9.28.0",
    "prettier": "3.5.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "style-loader": "^4.0.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@foxglove/schemas": "^1.7.2",
    "css-loader": "^7.1.2"
